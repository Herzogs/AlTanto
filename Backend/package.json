{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend del proyecto final de carrera - Al Tanto",
  "scripts": {
    "start": "ts-node src/server.ts",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --env-file=.env src/server.ts",
    "test": "cross-env NODE_ENV=test PORT=3000 jest --runInBand --forceExit --coverage",
    "test:watch": "cross-env NODE_ENV=test PORT=3000 jest --watchAll --runInBand"
  },
  "keywords": [],
  "author": "Equipo 5mentarios",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "jest": "29.7.0",
    "supertest": "7.0.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.19.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
